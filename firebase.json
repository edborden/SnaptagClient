{
  "firebase": "stalkers",
  "public": "./dist",
  "rewrites": [
    {
      "source": "/tests",
      "destination": "/tests/index.html"
    },
    {
      "source": "/tests/**",
      "destination": "/tests/index.html"
    },
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ]
}