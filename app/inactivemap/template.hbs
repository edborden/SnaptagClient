{{#if session.inactive}}
	<div class="circle-button second blue" {{action "join"}}>
		<div class="self-center">
			<i class="fa fa-eye fa-2x"></i>
			<br>
			<span class="small">Join</span>
		</div>
	</div>
{{else}}
	<div class="circle-button second blue" {{action "unjoin"}}>
		<div class="self-center">
			<i class="fa fa-minus-square-o fa-2x"></i>
			<br>
			<span class="small">Exit Queue</span>
		</div>
	</div>
{{/if}}
		
{{map-interface length=model.length}}
{{view 'intro-map'}}