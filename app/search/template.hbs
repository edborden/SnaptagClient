{{#snaptag-map bounds=initialBounds initialCenter=center initialZoom=zoom}}
  {{tile-layer url='https://{s}.tiles.mapbox.com/v3/edborden.i7622aec/{z}/{x}/{y}.png'}}
  {{me-marker}}
  {{#each model as |zone|}}
    {{circle-layer radius=zone.range lat=zone.lat lng=zone.lng color='blue' fillOpacity=0}}
    {{#each zone.users as |user|}}
      {{#marker-layer lat=user.location.lat lng=user.location.lng}}
        Active player who has found {{user.targetsFoundCount}} targets and has been playing since {{moment-from-now user.activatedAt}}.
      {{/marker-layer}}
    {{/each}}
  {{/each}}
{{/snaptag-map}}

{{search-interface length=model.length}}