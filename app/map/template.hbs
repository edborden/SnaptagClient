{{map-interface}}
{{#stalkers-map bounds=initialBounds center=center zoom=zoom}}
  {{tile-layer url='https://{s}.tiles.mapbox.com/v3/edborden.i7622aec/{z}/{x}/{y}.png'}}
  {{me-marker}}
  {{#each model as |location|}}
    {{#marker-layer lat=location.lat lng=location.lng}}
      {{location.user.name}}, {{moment-from-now location.createdAt}}.
    {{/marker-layer}}
  {{/each}}
{{/stalkers-map}}